        {
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH time_buckets AS (\r\n    SELECT generate_series(\r\n        date_trunc('minute', NOW() - INTERVAL '6 hours'),\r\n        date_trunc('minute', NOW()),\r\n        INTERVAL '1 minute'  -- ← Changed to 1-minute intervals\r\n    ) as time_bucket\r\n),\r\norder_counts AS (\r\n    SELECT \r\n        DATE_TRUNC('minute', timestamp) as time_bucket,  -- ← Simplified for 1-minute grouping\r\n        COUNT(*) as order_count\r\n    FROM transaction_data \r\n    WHERE timestamp >= NOW() - INTERVAL '6 hours'\r\n    GROUP BY time_bucket\r\n)\r\nSELECT \r\n    tb.time_bucket as time,\r\n    COALESCE(oc.order_count, 0) as \"order\"\r\nFROM time_buckets tb\r\nLEFT JOIN order_counts oc ON tb.time_bucket = oc.time_bucket\r\nORDER BY tb.time_bucket\r\n",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
